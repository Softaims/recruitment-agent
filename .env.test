# Test Environment Configuration
DATABASE_URL="postgresql://postgres:password@localhost:5432/recruitment_agent_test?schema=public"
NODE_ENV=test
PORT=3001

# JWT Configuration for testing
JWT_SECRET=test-jwt-secret-for-testing-only
JWT_EXPIRES_IN=7d

# Redis Configuration (use different DB for tests)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=1

# Disable external services in tests
OPENROUTER_API_KEY=test-key
TAVILY_API_KEY=test-key
VECTOR_DB_URL=http://localhost:19530
VECTOR_DB_API_KEY=

# Session Configuration
SESSION_TIMEOUT_MINUTES=30
MAX_CONVERSATION_HISTORY=100

# Rate Limiting (more lenient for tests)
RATE_LIMIT_TTL=60
RATE_LIMIT_LIMIT=1000

# Monitoring
LOG_LEVEL=error
ENABLE_METRICS=false