# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/recruitment_agent?schema=public"

# Application Configuration
NODE_ENV=development
PORT=3000

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# LLM Configuration
OPENROUTER_API_KEY=your-openrouter-api-key
DEFAULT_LLM_MODEL=openai/gpt-4

# Search Configuration
TAVILY_API_KEY=your-tavily-api-key

# Vector Database Configuration (Milvus/Pinecone)
VECTOR_DB_URL=http://localhost:19530
VECTOR_DB_API_KEY=

# Session Configuration
SESSION_TIMEOUT_MINUTES=30
SESSION_DEFAULT_EXPIRATION_HOURS=24
SESSION_CACHE_EXPIRATION_SECONDS=3600
SESSION_MAX_ACTIVE_SESSIONS=10
SESSION_CLEANUP_INTERVAL_MINUTES=60
MAX_CONVERSATION_HISTORY=100

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_LIMIT=100

# Monitoring
LOG_LEVEL=info
ENABLE_METRICS=true